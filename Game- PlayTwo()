
import java.util.List;
import java.util.ArrayList;
public class Game 
{
private 
private ArrayList<Card> deck = new ArrayList<Card>();
	private void playTwo(ArrayList<Card> playerHand)
	{
		if(player.buttonClick() == true )
	    {
			for(int i=0; i< playerHand.size(); i++)
			{
				if(playerHand.get(i).getValue() == 2)
				{
					playerHand.remove(i);
					break;
				}
			}
	    }
	}
}
