

public class Card implements Comparable<Card>
{
	private int suit;
    private int number;
    
    //1 is diamonds
    //2 is clubs
    //3 is spades
    //4 is hearts

   
    public Card(int mySuit, int myNumber)
    {
        suit = mySuit;
        number = myNumber;
    }
    
    public int getSuit()
    {
        return suit;
    }
    
    public int getNumber()
    {
        return number;
    }
    
    public int compareTo(Card other)
    {
        if(getNumber() > ((Card)other).getNumber())
            return 1;
        else if( getNumber() == ((Card)other).getNumber())
            return 0;
        else 
            return -1;
    }
    
    public int 
}

	
	

